{
  "name": "@bokub/linky",
  "description": "Easily retrieve your Linky power consumption",
  "version": "0.2.1",
  "author": "bokub",
  "bugs": "https://github.com/bokub/linky/issues",
  "dependencies": {
    "axios": "^0.18.0",
    "dayjs": "^1.8.0"
  },
  "devDependencies": {
    "ava": "^1.2.0",
    "codecov": "^3.1.0",
    "nyc": "^13.1.0",
    "xo": "^0.23.0"
  },
  "engines": {
    "node": ">= 7.6.0"
  },
  "files": [
    "index.js"
  ],
  "homepage": "https://github.com/bokub/linky",
  "keywords": [
    "linky"
  ],
  "license": "GPL-3.0-or-later",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/bokub/linky",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "test": "xo && nyc ava -v -s --fail-fast"
  },
  "xo": {
    "rules": {
      "no-prototype-builtins": 0,
      "unicorn/catch-error-name": 0,
      "camelcase": 0,
      "unicorn/prefer-type-error": 0,
      "capitalized-comments": 0
    }
  }
}
